<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seller.catterpiller">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/clientapplogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/applogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"></activity>

        <activity android:name=".DashBoardActivity"></activity>

        <activity android:name=".loginmodule.LoginActivity"
            android:windowSoftInputMode="stateVisible|adjustPan"
            ></activity>

        <activity android:name=".signupmodule.SignUpActivity"
            android:windowSoftInputMode="stateVisible|adjustPan"
            ></activity>

        <activity android:name=".WebviewActivity" />
        <activity android:name=".mybalance.BalanceActivity" />
        <activity android:name=".myaccount.AccountActivityDetail" />
        <activity android:name=".myaccount.AccountActivity" />
        <activity android:name=".payment.PaymentActivity" />
        <activity android:name=".ServicesActivity" />
        <activity android:name=".resetpassword.ResetPasswordActivity" />
        <activity android:name=".payment.PaymentHistoryActivity" />
        <activity android:name="com.seller.FaqActivity" />
    </application>

</manifest>